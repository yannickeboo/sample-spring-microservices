 pipeline {
    agent any
    stages {
        stage('Build') { 
            steps {
                sh 'mvn -B -DskipTests clean package' 
            }
        }
     stage('docker image'){
       sh '''
                                docker build -t yannickeboo:${VERSION} .
                                docker login -u yannickeboo -p $docker_yann
                              
                            '''
    }
}
